    "formatcheck": "pnpm exec check-formatting",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "‚òùÔ∏è required package scripts": "",
    "prepublishOnly": "pnpm -w exec turbo run build --filter=\"@dopt/resolve-workspace-dependencies\""
  },
  "files": [
    "cli.js",
    "dist"
  ],
  "dependencies": {
    "@dopt/wutils": "workspace:*",
