  },
  "devDependencies": {
    "@dopt/check-formatting": "workspace:*",
    "@dopt/config": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "esbuild": "0.18.2",
    "esbuild-jest": "0.5.0",
    "eslint": "8.42.0",
    "eslint-plugin-react": "7.32.2",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "shx": "0.3.4",
    "typescript": "5.1.6",
    "unbuild": "2.0.0"
  },
  "openSource": true
}
