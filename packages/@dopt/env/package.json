{
  "name": "@dopt/env",
  "version": "0.0.0",
  "private": true,
  "description": "A CLI for generating packages in the Dopt monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "üëárequired package scripts": "",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "clean": "shx rm -rf ./dist",
    "dev": "pnpm run build --watch src",
    "format": "echo \"@dopt/env does not require format\"; exit 0;",
    "formatcheck": "echo \"@dopt/env does not require formatcheck\"; exit 0;",
    "lint": "echo \"@dopt/env does not require lint\"; exit 0;",
    "test": "echo \"@dopt/env does not require lint\"; exit 0;",
    "typecheck": "tsc --noEmit",
    "‚òùÔ∏è required package scripts": ""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dopt/config": "workspace:*",
    "@types/node": "18.11.5",
    "esbuild": "0.15.10",
    "eslint": "8.24.0",
    "shx": "0.3.4",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  }
}
