{
  "name": "@dopt/react",
  "opensource": true,
  "version": "0.0.9",
  "description": "An SDK for accessing and transitioning a Journey's Model Blocks in Dopt",
  "license": "MIT",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "👇required package scripts": "",
    "build": "rollup -c; rm -rf ./dist/types",
    "clean": "shx rm -rf ./dist",
    "dev": "rollup -cw --no-watch.clearScreen",
    "format": "prettier --w .",
    "formatcheck": "pnpm exec check-formatting",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "☝️ required package scripts": ""
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@dopt/check-formatting": "workspace:*",
    "@dopt/config": "workspace:*",
    "@rollup/plugin-typescript": "8.3.3",
    "@swc/core": "1.2.237",
    "@types/jest": "29.0.3",
    "@types/react": "18.0.17",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "esbuild": "0.15.0",
    "esbuild-jest": "0.5.0",
    "eslint": "8.21.0",
    "eslint-plugin-react": "7.30.1",
    "jest": "29.0.0",
    "prettier": "2.7.1",
    "rollup": "2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "shx": "0.3.4",
    "tslib": "^2.4.0",
    "tsup": "6.2.1",
    "typescript": "4.7.4"
  }
}
