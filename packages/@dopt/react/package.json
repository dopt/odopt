{
  "name": "@dopt/react",
  "version": "0.0.9",
  "private": false,
  "description": "An SDK for accessing and transitioning a Journey's Model Blocks in Dopt",
  "license": "MIT",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "👇required package scripts": "",
    "build": "dotenv-flow -- rollup --config rollup.config.ts --configPlugin typescript; rm -rf ./dist/types",
    "clean": "shx rm -rf ./dist",
    "dev": "rollup -cw --no-watch.clearScreen",
    "format": "prettier --w .",
    "formatcheck": "pnpm exec check-formatting",
    "lint": "eslint .",
    "test": "dotenv-flow -- jest",
    "typecheck": "tsc --noEmit",
    "☝️ required package scripts": "",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm test"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@dopt/check-formatting": "workspace:*",
    "@dopt/config": "workspace:*",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/jest": "29.2.0",
    "@types/react": "18.0.21",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.24.0",
    "eslint-plugin-react": "7.31.8",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "rollup": "3.2.2",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "shx": "0.3.4",
    "tslib": "^2.4.0",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  }
}
