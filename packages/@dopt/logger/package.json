{
  "name": "@dopt/logger",
  "version": "0.0.1",
  "description": "A logger package with configuration to set severity loglevel for your standard console logs",
  "private": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "üëárequired package scripts": "",
    "build": "rollup --config rollup.config.ts --configPlugin typescript; rm -rf ./dist/types",
    "clean": "shx rm -rf ./dist",
    "dev": "rollup -w --config rollup.config.ts --configPlugin typescript --no-watch.clearScreen",
    "format": "prettier --w .",
    "formatcheck": "pnpm exec check-formatting",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "‚òùÔ∏è required package scripts": "",
    "prepublishOnly": "pnpm test; pnpm build;"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "4.1.2",
    "loglevel": "1.8.0"
  },
  "devDependencies": {
    "@dopt/check-formatting": "workspace:*",
    "@dopt/config": "workspace:*",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/jest": "29.2.1",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "esbuild": "0.15.10",
    "esbuild-jest": "0.5.0",
    "eslint": "8.24.0",
    "eslint-plugin-react": "7.31.8",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "rollup": "3.2.2",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "shx": "0.3.4",
    "tslib": "^2.4.0",
    "typescript": "4.8.4"
  }
}
