{
  "name": "@dopt/block-api-types",
  "version": "0.0.1",
  "description": "A package of types corresponding to our Block API",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "👇required package scripts": "",
    "build": "unbuild",
    "clean": "shx rm -rf ./dist",
    "dev": "unbuild --stub",
    "format": "prettier --w .",
    "formatcheck": "pnpm exec check-formatting",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "☝️ required package scripts": ""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dopt/check-formatting": "workspace:*",
    "@dopt/config": "workspace:*",
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "esbuild": "0.17.12",
    "esbuild-jest": "0.5.0",
    "eslint": "8.36.0",
    "eslint-plugin-react": "7.32.2",
    "jest": "29.5.0",
    "prettier": "2.8.6",
    "shx": "0.3.4",
    "typescript": "4.9.5",
    "unbuild": "1.1.2"
  },
  "dependencies": {
    "@userstate/types": "workspace:*"
  }
}
